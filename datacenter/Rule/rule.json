          "RemoveChar": {
            "IsRegex": true,
            "Chars": [
              "<[a|A]\\s*[^>]*>(.*?)</[a|A]>",
              "<div class=\"read-more\">([\\s\\S]*)</div>",
              "<a[^>]+href=\"([^\"]*)\"[^>]*>([\\s\\S]*?)</a>"
