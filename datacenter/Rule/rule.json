          }
        }
      },
      {
        "Key": "author",
        "IsList": false,
        "Rule": {
          "XpathExtractModel": {
            "XpathRule": "//div[@class='media__title media__title_arcticle']",
            "ExtractType": "Text"
          }
        }
      }
    ]
  },
