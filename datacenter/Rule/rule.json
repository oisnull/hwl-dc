[
  {
    "Url": "https://edition.cnn.com/2019/02/13/world/china-india-greener-planet-scli-intl/index.html",
    "Rules": [
      {
        "Key": "title",
        "IsList": false,
        "Rule": {
          "XpathExtractModel": {
            "XpathRule": "//h1[@class='pg-headline']",
            "ExtractType": "Text"
          }
        }
      },
      {
        "Key": "summary",
        "IsList": false,
        "Rule": {
          "XpathExtractModel": {
            "XpathRule": "//div[@class='el__leafmedia el__leafmedia--sourced-paragraph']",
            "ExtractType": "Text"
          },
          "RemoveChar": {
            "IsRegex": false,
            "Chars": [
              "(CNN)"
            ]
          }
        }
      },
      {
        "Key": "publishdate",
        "IsList": false,
        "Rule": {
          "XpathExtractModel": {
            "XpathRule": "//p[@class='update-time']",
            "ExtractType": "Text"
          }
        }
      },
      {
        "Key": "keys",
        "IsList": false,
        "Rule": {
          "DefaultValue": "CNN NEW"
        }
      },
      {
        "Key": "imageurls",
        "IsList": true,
        "Rule": {
          "XpathExtractModel": {
            "XpathRule": "//section[@id='body-text']//img[@src]",
            "XpathEndAttributes": [
              "src"
            ]
          }
        }
      },
      {
        "Key": "content",
        "IsList": false,
        "Rule": {
          "XpathExtractModel": {
            "XpathRule": "//section[@id='body-text']"
          },
          "RemoveChar": {
            "IsRegex": true,
            "Chars": [
              "<[a|A]\\s*[^>]*>(.*?)</[a|A]>",
              "<cite\\s*[^>]*>(.*?)</cite>",
              "<div class=\"zn-body__read-more-outbrain\">.*</div>"
            ]
          }
        }
      },
      {
        "Key": "author",
        "IsList": false,
        "Rule": {
          "XpathExtractModel": {
            "XpathRule": "//span[@class='metadata__byline__author']",
            "ExtractType": "Text"
          }
        }
      }
    ]
  },
  {
